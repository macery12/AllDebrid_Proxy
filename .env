# ==========================
# Core
# ==========================
WORKER_API_KEY=dummy-worker-api-key
WORKER_KEY=dummy-worker-api-key
LOGIN_USERS=local:test;
WORKER_BASE_URL=http://adproxy_api:8080

# ==========================
# AllDebrid
# ==========================
ALLDEBRID_API_KEY=dummy-alldebrid-api-key
ALLDEBRID_AGENT=Generic-PC

# ==========================
# Storage
# ==========================
STORAGE_ROOT=/srv/storage
LOW_SPACE_FLOOR_GB=10

# ==========================
# Limits
# ==========================
ARIA2_RPC_URL=http://aria2:16800/jsonrpc
ARIA2_RPC_SECRET=dummy-secret
ARIA2_SPLITS=4
PER_TASK_MAX_ACTIVE=2

# ==========================
# Postgres
# ==========================
POSTGRES_USER=alldebrid
POSTGRES_PASSWORD=alldebrid
POSTGRES_DB=alldebrid
DATABASE_URL=postgresql+psycopg2://alldebrid:alldebrid@db:5432/alldebrid

# ==========================
# Redis
# ==========================
REDIS_URL=redis://redis:6379/0

# ==========================
# Frontend
# ==========================
FRONTEND_PORT=5000

# ==========================
# NGINX / Proxy
# ==========================
USE_X_ACCEL=1
NGINX_ACCEL_PREFIX=/protected
FILES_SUBDIR=files
